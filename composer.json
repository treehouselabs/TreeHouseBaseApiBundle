{
  "name": "treehouselabs/base-api-bundle",
  "type": "symfony-bundle",
  "description": "Base bundle for Api functionality",
  "keywords": [
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Kruithof",
      "email": "peter@treehouse.nl"
    },
    {
      "name": "Jeroen Fiege",
      "email": "jeroen@treehouse.nl"
    }
  ],
  "require": {
    "php": ">=5.5",
    "symfony/symfony": "^2.6",
    "jms/serializer": "^1.0"
  },
  "require-dev": {
    "doctrine/orm": "^2.3",
    "friendsofsymfony/user-bundle": "~2.0@dev",
    "phpunit/phpunit": "^4.1",
    "behat/behat": "^3.0",
    "behat/symfony2-extension": "^2.0"
  },
  "suggest": {
    "treehouselabs/keystone-bundle": "For API authentication that is easy to set up",
    "behat/behat": "To test the API using Behat",
    "behat/symfony2-extension": "To test the API using Behat",
    "doctrine/orm": "When you want to set up fixtures in Behat tests",
    "friendsofsymfony/user-bundle": "When you want to set up fixtures in Behat tests"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\BaseApiBundle\\": "src/TreeHouse/BaseApiBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TreeHouse\\BaseApiBundle\\Tests\\": "tests/TreeHouse/BaseApiBundle/Tests/"
    }
  }
}
